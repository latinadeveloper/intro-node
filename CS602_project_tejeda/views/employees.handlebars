<h3>Employees with Last Name {{{name}}}</h3>
<table>
  <thead>
    <tr><td>Id</td><td>First Name</td><td>Last Name</td></tr>
  </thead>
  <tbody>
    {{#each employees}} 
    	<tr>
        <td>{{{this.id}}}</td>
        <td>{{{this.firstName}}}</td>
        <td>{{{this.lastName}}}</td>
        <td>
          <a href="/employees/edit/{{this.id}}">Edit</a>
          <a href="/employees/delete/{{this.id}}">Delete</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
